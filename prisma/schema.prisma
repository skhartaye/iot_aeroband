generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model SensorData {
  id        Int      @id @default(autoincrement())
  value     Float
  timestamp DateTime @default(now())
  deviceId  String
  location  String?
  status    String?
  type      String
  unit      String
}
